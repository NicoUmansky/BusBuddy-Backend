{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }

    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.js"
      }
    ],
    "functions": {
      "api/*": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }